    <h3>I N D E X</h3>


    <div id="display-cards">
        <% books.forEach( book => { %>
        <div>
            <h3><%= book.title %></h3>
            <p>Published: <%= book.published %></p>
            <a href="/books/<%= book.id %>"></a>
            <img src="<%= book.coverImagePath %>" height="150" width="100" alt="<%= book.title %>" >
            </a>
            </div>
        <% }) %>
    </div>